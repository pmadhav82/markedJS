
<div class="container  ">
<a href="/newPost" class="btn btn-success"> All Articles</a>
{{#each post}}
<div class="card-body  border p-2 m-3">
     <a href="/editPost/{{this.id}}" class="btn btn-primary m-2">Edit</a>

        <a href="/deletePost/{{this.id}}" class="btn btn-secondary m-2">Delete</a>
<h2 class="card-title p-2 text-center">
    
{{this.title}}

</h2>
    
<div class="card-text m-3">

    {{{this.html}}}
</div>

</div>
{{/each}}
</div>
    





